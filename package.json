{
  "name": "outlook-smart-draft-addin",
  "version": "1.0.0",
  "description": "Outlook add-in for generating smart email drafts",
  "main": "index.js",
  "scripts": {
    "start": "office-addin-debugging start manifest.xml desktop",
    "stop": "office-addin-debugging stop manifest.xml",
    "serve": "http-server -p 3000 --ssl --cert ~/.office-addin-dev-certs/localhost.crt --key ~/.office-addin-dev-certs/localhost.key",
    "validate": "office-addin-manifest validate manifest.xml"
  },
  "dependencies": {
    "core-js": "^3.32.0",
    "regenerator-runtime": "^0.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-loader": "^9.1.2",
    "html-webpack-plugin": "^5.5.3",
    "office-addin-debugging": "^5.0.0",
    "office-addin-dev-certs": "^1.13.0",
    "office-addin-lint": "^2.3.0",
    "office-addin-manifest": "^1.13.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}